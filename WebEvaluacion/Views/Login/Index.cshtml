@section style {
    <link rel="stylesheet" href="/Libs/Dashboard/css/adminlte.min.css">
    @*<link href="/Admin/css/sistema.css" rel="stylesheet" />*@
}


<div class="hold-transition login-page">

    <div class="login-box">
        <!-- /.login-logo -->
        <div class="card">
            <div class="card-body login-card-body">
                <p class="login-box-msg">Iniciar Sesión</p>
                <form method="post" id="frmlogin" action="@Url.Action("Login", "Login")">

                    <div class="text-center mb-3">
                        <img src="data:image/jpeg;base64," class="w-100">
                    </div>

                    <!-- USUARIO -->
                    <div class="input-group mt-3">
                        <input type="text" class="form-control mayus credenciales" placeholder="Usuario" id="txtUsuario" value="" name="usuario" autofocus required>
                        <div class="input-group-append input-group-text">
                            <span class="fas fa-user"></span>
                        </div>
                    </div>
                    <!-- CLAVE -->
                    <div class="input-group mt-3">
                        <input type="password" class="form-control mayus credenciales" placeholder="Contraseña" id="txtPassword" name="clave" value="" required>
                        <div class="input-group-append input-group-text">
                            <span class="fas fa-lock"></span>
                        </div>
                    </div>

                    <div class="input-group mt-3">
                        <button href="#" class="btn btn-block btn-primary" id="btnLogin" type="submit">
                            Login
                        </button>
                    </div>

                </form>
                @*<div class="social-auth-links text-center mt-3">

                </div>*@
                <!-- /.social-auth-links -->
            </div>
            <!-- /.login-card-body -->
        </div>
    </div>
</div>


@section scripts {

    <script>
        $("#navbarid").addClass("d-none");

        const frmlogin = document.getElementById("frmlogin");

        frmlogin.addEventListener('submit',async (e) => {
            e.preventDefault();
            let parametros = {
                usuario: $("#txtUsuario").val(),
                clave: $("#txtPassword").val(),

            }

            let response = await post('Login', 'Login', parametros, true);
            if (response.idUsuario == 0) {
                alert("Usuario no existe");
            } else {
                location.reload();
            }

        })

    </script>


}